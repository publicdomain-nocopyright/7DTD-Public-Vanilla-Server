<configs>
<append xpath="/controls">


	<lesssimplebutton disableautobackground="[disableautobackground]" text="[text]" text_key="[text_key]" background="[background]" texture="[texture]"  style="press, hover"   depth="3" pos="[pos]" width="200" height="32"
			enabled_font_color="[labelColor]" disabled_font_color="[disabledLabelColor]" visible="true" enabled="true" bordersprite="menu_empty2px" bordercolor="[bordercolor]" pivot="left"
			defaultsprite="menu_empty" defaultcolor="[mediumGrey]" hoversprite="ui_game_select_row" hovercolor="[white]" selectedsprite="ui_game_select_row" selectedcolor="[mediumGrey]"
			foregroundlayer="true" tooltip_key=""  justify="center" caption="" caption_key="" font_size="28" upper_case="false" parse_actions="false" >
		<rect style="press, hover" name="${name}" depth="${depth}" pos="${pos}" width="${width}" height="${height}" enabled_font_color="${enabled_font_color}" disabled_font_color="${disabled_font_color}" controller="ServerInfo" visible="${visible}" button_enabled="${enabled}">
			<!-- <rect   width="286" height="286" pos="-43,43" ></rect>-->
			
				
				<texture pos="0,0"  style="press, hover" sound_play_on_hover="[default_hover]"  name="randtextu" width="${width}" height="${height}" on_press="true" on_hover="true" depth="5"  texture="${texture}" material="Materials/Transparent Colored" />
				
				
				<!--Works but glitched 
				<texture pos="-43,43"  style="press, hover" sound_play_on_hover="[default_hover]"  name="randtextu" width="${width}" height="${height}" on_press="true" on_hover="true" depth="5"  texture="${texture}" material="Materials/Transparent Colored" />
			-->
			<!--Image button -->
			<!--<texture style="press, hover" sound_play_on_hover="[default_hover]"  name="ExitScreena4" width="100%" height="100%" on_press="true" on_hover="true" depth="5"   pos="0,0" texture="@https://i.imgur.com/oZ3Xjoj.png" material="Materials/Transparent Colored" />-->
			<!-- <sprite style="press, hover" depth="9" name="background" color="[red]" type="sliced" on_press="true" on_hover="true" width="147" sound_play_on_hover="[default_hover]"/>-->
			


			<label name="ServerDescription" />
			<!-- Hover buggy, but click sound is alright here. style="press, hover"       Setting as foreground layer didn't work:     depth="1"   foregroundlayer="true" -->
				<label sound="[recipe_click]" sound_play_on_hover="[default_hover]" depth="10" pos="0,0" width="${width}" height="${height}"  name="ServerWebsiteURL" text="${text}" text_key="${text_key}" justify="center" style="press" color="237, 41, 57,1" font_size="30" upper_case="false" button_name="link2hover"/>
			
			<label depth="2" name="btnLabel" width="${width}" height="${height-2}" pos="0,${Round(-height/2,0)}" justify="${justify}" text="${caption}" text_key="${caption_key}" effect="outline" pivot="${pivot}" font_size="${font_size}" upper_case="${upper_case}" parse_actions="${parse_actions}" />
			

			
			<!-- foregroundlayer="true" -->
			<sprite  disableautobackground="${disableautobackground}" depth="1"  on_press="true" on_hover="true" sound_play_on_hover="[default_hover]" name="background" width="100%" height="100%" pos="0,0" color="${background}" type="sliced" foregroundlayer="true" />
			<sprite depth="2" name="border" sprite="${bordersprite}" pos="0,${Round(-height/2,0)}" color="${bordercolor}" type="sliced" fillcenter="false" foregroundlayer="true" pivot="${pivot}" />	


			
			<!--
			<button name="clickable" pos="0,${Round(-height/2,0)}" sprite="${defaultsprite}" defaultcolor="${defaultcolor}" disabledcolor="[darkGrey]" hoversprite="${hoversprite}" hovercolor="${hovercolor}" selectedsprite="${selectedsprite}" selectedcolor="${selectedcolor}"  type="sliced" hoverscale="1.0" pivot="${pivot}" tooltip_key="${tooltip_key}" sound_play_on_hover="[default_hover]" sound="${sound}" > 
			
			<label depth="2" name="btnLabel" width="${width}" height="${height-2}" pos="0,${Round(-height/2,0)}" justify="${justify}" text="${caption}" text_key="${caption_key}" effect="outline" pivot="${pivot}" font_size="${font_size}" upper_case="${upper_case}" parse_actions="${parse_actions}" />

			<label name="ServerDescription" />
			<label foregroundlayer="true"  depth="10" pos="-100,90" width="300" height="300"  name="ServerWebsiteURL" text="testza" text_key="link2url" justify="center" style="press" color="0,0,0,1" font_size="300" upper_case="false" button_name="link2hover"/>

			</button>
			 -->
			
			
			<!-- <label name="ServerWebsiteURL" text_key="link1url" style="press"  upper_case="false" button_name="link1hover" depth="1" width="${width}" height="${height-2}" pos="0,${Round(-height/2,0)}"      justify="${justify}" text="${caption}"  effect="outline" pivot="${pivot}" font_size="${font_size}"  parse_actions="${parse_actions}"/> -->


		</rect>
	</lesssimplebutton>
</append>
</configs>